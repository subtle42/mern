{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "run-p dev:*",
    "dev:client": "webpack --watch --mode=development",
    "dev:server": "nodemon --inspect=5858 -r ts-node/register ./server/app.ts",
    "test": "run-p 'test:* -- --watch'",
    "test:client": "mocha --opts ./mocha.opts 'client/**/*.spec.*'",
    "test:server": "mocha --opts ./mocha.opts 'server/**/*.spec.*'",
    "integration": "mocha --opts ./mocha.opts 'integration/**/*.spec.*' --watch",
    "e2e": "wdio",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "coverage": "nyc --all run-s 'build:test:*'",
    "docs": "typedoc --exclude '**/*.spec.*' --out ./docs ./client/data ./server ./common",
    "build:code": "run-p build:code:*",
    "build:code:client": "webpack --mode=production",
    "build:code:server": "tsc -p server",
    "build:test:unit": "run-s 'test:*'",
    "build:test:integration": "mocha --opts ./mocha.opts 'integration/**/*.spec.*' --exit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "css-loader": "^2.1.1",
    "d3": "^5.9.2",
    "express": "^4.16.4",
    "fast-csv": "^2.4.1",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.0.2",
    "mongodb": "^3.2.2",
    "mongoose": "^5.4.19",
    "multer": "^1.4.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-dropzone": "^10.0.6",
    "react-fontawesome": "^1.6.1",
    "react-grid-layout": "^0.16.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.3.0",
    "socket.io": "^2.2.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.4000",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-livereload-plugin": "^2.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/d3": "^5.7.1",
    "@types/enzyme": "^3.9.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.22",
    "@types/mongoose": "^5.3.23",
    "@types/multer": "^1.3.7",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-grid-layout": "^0.16.7",
    "@types/react-loadable": "^5.5.0",
    "@types/react-redux": "^7.0.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^7.1.3",
    "@types/redux": "^3.6.0",
    "@types/sinon": "^7.0.10",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/webdriverio": "^5.0.0",
    "@types/winston": "^2.4.4",
    "chai-webdriverio": "^1.0.0",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "selenium-standalone": "^6.16.0",
    "source-map-support": "^0.5.11",
    "superagent": "^4.1.0",
    "tsconfig-paths": "^3.8.0",
    "typedoc": "^0.14.2",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^5.7.5",
    "webpack-cli": "^3.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      "/**/*.spec.*",
      "./e2e",
      "./integration",
      "./client"
    ],
    "ext": "ts"
  },
  "nyc": {
    "include": [
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "./integration/**/*",
      "./common/**/*",
      "./e2e/**/*",
      "**/*.spec.*",
      "**/model.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
