{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "run-p dev:*",
    "dev:client": "webpack --watch --mode=development",
    "dev:server": "nodemon --inspect=5858 ./server/app.ts",
    "test": "run-p 'test:* -- --watch'",
    "test:client": "mocha --opts ./mocha.opts 'client/**/*.spec.*'",
    "test:server": "mocha --opts ./mocha.opts 'server/**/*.spec.*'",
    "integration": "mocha --opts ./mocha.opts 'integration/**/*.spec.*' --watch",
    "e2e": "wdio",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "coverage": "nyc --all run-s 'build:test:*'",
    "docs": "typedoc --exclude '**/*.spec.*' --out ./docs ./client/data ./server ./common",
    "build:code": "run-p build:code:*",
    "build:code:client": "webpack --mode=production",
    "build:code:server": "tsc -p server",
    "build:test:unit": "run-s 'test:*'",
    "build:test:integration": "mocha --opts ./mocha.opts 'integration/**/*.spec.*' --exit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "bootstrap": "^4.1.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "css-loader": "^0.28.11",
    "d3": "^4.13.0",
    "express": "^4.16.3",
    "fast-csv": "^2.4.1",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^4.1.0",
    "mongodb": "^3.1.0",
    "mongoose": "^5.1.7",
    "multer": "^1.3.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^4.2.12",
    "react-fontawesome": "^1.6.1",
    "react-grid-layout": "^0.16.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^5.0.0",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.3.0",
    "sinon": "^4.5.0",
    "socket.io": "^2.1.1",
    "style-loader": "^0.19.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^2.8.3",
    "url-loader": "^0.6.2",
    "webpack": "^4.12.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-livereload-plugin": "^2.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.21",
    "@types/chai": "^4.1.4",
    "@types/chai-http": "^3.0.4",
    "@types/d3": "^4.13.0",
    "@types/enzyme": "^3.1.12",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^2.2.48",
    "@types/mongodb": "^3.1.1",
    "@types/mongoose": "^4.7.37",
    "@types/multer": "^1.3.7",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.4.3",
    "@types/react-dom": "^16.0.6",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-fontawesome": "^1.6.3",
    "@types/react-grid-layout": "^0.16.4",
    "@types/react-loadable": "^5.4.1",
    "@types/react-redux": "^5.0.20",
    "@types/react-router-dom": "^4.2.7",
    "@types/reactstrap": "^5.0.27",
    "@types/redux": "^3.6.0",
    "@types/redux-promise": "^0.5.28",
    "@types/sinon": "^4.3.3",
    "@types/socket.io": "^1.4.36",
    "@types/socket.io-client": "^1.4.32",
    "@types/webdriverio": "^4.10.3",
    "chai-webdriverio": "^0.4.3",
    "coveralls": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.9.0",
    "selenium-standalone": "^6.15.1",
    "source-map-support": "^0.5.6",
    "superagent": "^3.8.3",
    "tsconfig-paths": "^3.4.2",
    "typedoc": "^0.11.1",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.13.1",
    "webpack-cli": "^2.1.5"
  },
  "nodemonConfig": {
    "ignore": [
      "/**/*.spec.*",
      "./e2e",
      "./integration",
      "./client"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node -P server"
  },
  "nyc": {
    "include": [
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "./integration/**/*",
      "./common/**/*",
      "./e2e/**/*",
      "**/*.spec.*",
      "**/model.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
