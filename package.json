{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "run-p dev:*",
    "dev:client": "webpack --watch --mode=development",
    "dev:server": "nodemon --inspect=5858 ./server/app.ts",
    "test": "run-p 'test:* -- --watch'",
    "test:client": "mocha --opts ./mocha.opts 'client/**/*.spec.*'",
    "test:server": "mocha --opts ./mocha.opts 'server/**/*.spec.*'",
    "integration": "mocha --opts ./mocha.opts 'integration/**/*.spec.*' --watch",
    "e2e": "wdio",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "coverage": "nyc --all run-s 'build:test:*'",
    "docs": "typedoc --exclude '**/*.spec.*' --out ./docs ./client/data ./server ./common",
    "build:code": "run-p build:code:*",
    "build:code:client": "webpack --mode=production",
    "build:code:server": "tsc -p server",
    "build:test:unit": "run-s 'test:*'",
    "build:test:integration": "mocha --opts ./mocha.opts 'integration/**/*.spec.*' --exit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.2.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.4",
    "fast-csv": "^2.4.1",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^4.1.0",
    "mongodb": "^3.1.10",
    "mongoose": "^5.4.0",
    "multer": "^1.4.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-dropzone": "^4.3.0",
    "react-fontawesome": "^1.6.1",
    "react-grid-layout": "^0.16.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^5.0.0",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.3.0",
    "sinon": "^4.5.0",
    "socket.io": "^2.2.0",
    "style-loader": "^0.19.1",
    "ts-loader": "^4.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.3.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-livereload-plugin": "^2.1.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.25",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/enzyme": "^3.1.15",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mocha": "^2.2.48",
    "@types/mongodb": "^3.1.17",
    "@types/mongoose": "^4.7.40",
    "@types/multer": "^1.3.7",
    "@types/passport": "^0.4.7",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-grid-layout": "^0.16.5",
    "@types/react-loadable": "^5.4.2",
    "@types/react-redux": "^5.0.21",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^5.0.27",
    "@types/redux": "^3.6.0",
    "@types/redux-promise": "^0.5.28",
    "@types/sinon": "^4.3.3",
    "@types/socket.io": "^1.4.40",
    "@types/socket.io-client": "^1.4.32",
    "@types/webdriverio": "^4.13.0",
    "@types/winston": "^2.4.4",
    "chai-webdriverio": "^0.4.3",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.9.0",
    "selenium-standalone": "^6.15.4",
    "source-map-support": "^0.5.9",
    "superagent": "^3.8.3",
    "tsconfig-paths": "^3.7.0",
    "typedoc": "^0.11.1",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.1",
    "webpack-cli": "^3.1.2"
  },
  "nodemonConfig": {
    "ignore": [
      "/**/*.spec.*",
      "./e2e",
      "./integration",
      "./client"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node"
  },
  "nyc": {
    "include": [
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "./integration/**/*",
      "./common/**/*",
      "./e2e/**/*",
      "**/*.spec.*",
      "**/model.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
